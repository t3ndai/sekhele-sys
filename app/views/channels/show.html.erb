<p style="color: green"><%= notice %></p>
<%= render @channel %>
<div>
  <%= link_to "New Post", new_channel_channel_post_path(@channel) %>
</div>
<div>
  <%= form_with url: join_channel_path, method: :post do |form| %>
    <%= form.submit "Join Channel" %>
  <% end %>
</div>
<div>
  <%= link_to "Edit this channel", edit_channel_path(@channel) %> |
  <%= button_to "Destroy this channel", @channel, method: :delete %>
</div>
