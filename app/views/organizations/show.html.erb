<%= render @organization %>
<div>
  <%= link_to "Edit this organization", edit_organization_path(@organization) %> |
  <%= link_to "Back to organizations", organizations_path %>
  <%= link_to "Add Employee", new_organization_employee_path(@organization) %>
  <%= link_to "Add Location", new_organization_location_path(@organization) %>
  <%= link_to "Add Cost Center", new_organization_cost_center_path(@organization) %>
  <%= button_to "Destroy this organization", @organization, method: :delete %>
</div>
