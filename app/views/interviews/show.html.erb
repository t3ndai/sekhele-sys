<%= render @interview %>
<div>
  <%= link_to "Assign Interviewer(s)", new_interview_interviewer_path(@interview) %>
</div>
<% if @interview.interviewers.exists?(Current.user&.employee.id) %>
  <div>
    <%= link_to "New interview feedback", new_interview_interview_feedback_path(@interview) %>
  </div>
<% end %>
<div>
  <%= link_to "Edit this interview", edit_interview_path(@interview) %> |
  <%= link_to "Back to interviews", job_applicant_interviews_path(@interview.job_applicant) %>
  <%= button_to "Destroy this interview", @interview, method: :delete %>
</div>
